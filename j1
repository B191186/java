import java.util.*;
class array {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int i,j;
        int k=0;
        int start;
        int end;
        int a[]={1,2,3,4};
        int n=a.length;
        for(i=0;i<n;i++){
            start=i;
            for(j=i;j<n;j++){
            end=j;
            for(k=start;k<=end;k++){
                System.out.print(+a[k]);
            }
            System.out.println( );
        }
    }
}
}
